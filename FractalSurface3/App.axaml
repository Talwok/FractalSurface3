<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="FractalSurface3.App"
             xmlns:local="using:FractalSurface3"
             xmlns:materialIconsAvalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             xmlns:fluentAvaloniaStyling="clr-namespace:FluentAvalonia.Styling;assembly=FluentAvalonia"
             RequestedThemeVariant="Default">
    
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <fluentAvaloniaStyling:FluentAvaloniaTheme PreferSystemTheme="True" PreferUserAccentColor="True"/>
        <materialIconsAvalonia:MaterialIconStyles/>
    </Application.Styles>
             
</Application>